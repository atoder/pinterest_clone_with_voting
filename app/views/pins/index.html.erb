<div id="pins" class="transitions-enabled masonry">
  <% @pins.each do |pin| %>
    <div class="box panel panel-default">
      <div class="panel-body">
      <%= link_to pin do %>
        <p><%= image_tag pin.image.url %></p>
      <% end %>
        <p><%= pin.title %></p>
      </div>
    </div>
  <% end %>
</div>

